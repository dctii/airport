<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Example Suite" verbose="1" parallel="tests" thread-count="10">
    <listeners>
        <listener class-name="com.solvd.airport.listeners.TestNGListener"/>
    </listeners>

    <test name="ExampleTest1" parallel="methods" thread-count="5">
        <classes>
            <class name="com.solvd.airport.ExampleTest">
                <methods>
                    <exclude name="excludeMe"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="ExampleTest2" parallel="methods" thread-count="5">
        <classes>
            <class name="com.solvd.airport.ExampleTest">
                <methods>
                    <exclude name="excludeMe"/>
